<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.sabaid.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_akun"
        android:name="com.example.sabaid.ui.dashboard.AkunFragment"
        android:label="@string/title_akun"
        tools:layout="@layout/fragment_akun" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.sabaid.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifikasi"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_menu"
        android:name="com.example.sabaid.NotifikasiNewsFragment"
        android:label="fragment_notifikasi_news"
        tools:layout="@layout/fragment_notifikasi_news" />
